<% content_for :title, 'Edit time entry' %>

<%= render :partial => 'partials/form_errors', :locals => { :model => @time_entry } %>

<%= form_for :time_entry, :url => user_time_entry_path(@current_user, @time_entry),
  :html => { :method => 'put' } do |form| %>

  <%= render :partial => 'form', :locals => { :form => form } %>

  <%= submit_tag "Submit", :class => 'btn' %>
  &nbsp;
  <%= link_to 'Cancel', user_time_entries_path(@current_user) %>

<% end %>

