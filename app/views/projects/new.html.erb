<% content_for :title, 'Create new project' %>

<%= render :partial => 'partials/form_errors', :locals => { :model => @project } %>

<%= form_for :project, :url => user_projects_path(@current_user) do |form| %>

  <%= render :partial => 'form', :locals => { :form => form } %>

  <%= submit_tag "Submit", :class => 'btn' %>
  &nbsp;
  <%= link_to 'Cancel', user_projects_path(@current_user) %>

<% end %>

